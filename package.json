{
  "name": "crett-ui-kit",
  "version": "0.1.0",
  "description": "It is a simple start with a profile user card, that is ment to grow into a reusable React UI components library. Those everyone can use in their projects for free. All the components are built using styled-components and tested with Storybook.",
  "files": [
    "dist"
  ],
  "main": "./dist/crett-ui-kit.umd.cjs",
  "module": "./dist/crett-ui-kit.js",
  "exports": {
    ".": {
      "import": "./dist/crett-ui-kit.js",
      "require": "./dist/crett-ui-kit.umd.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublish": "npm run build"
  },
  "resolutions": {
    "styled-components": "^6.1.19"
  },
  "plugins": [
    "babel-plugin-styled-components"
  ],
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "styled-components": "^6.1.19",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@tsconfig/recommended": "^1.0.10",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.1.9",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^9.1.10",
    "globals": "^16.4.0",
    "playwright": "^1.55.1",
    "prop-types": "^15.8.1",
    "storybook": "^9.1.10",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
